  <Capability Min="104" Max="107">LEDs on</Capability>
  <Capability Min="108" Max="207">Impulse effect, increasing speed (dimmed light and dark)</Capability>
  <Capability Min="208" Max="212">LEDs on</Capability>
  <Capability Min="213" Max="225">Strobe effect (low speed)</Capability>
  <Capability Min="226" Max="238">Strobe effect (medium speed)</Capability>
  <Capability Min="239" Max="251">Strobe effect (high speed)</Capability>
  <Capability Min="252" Max="255">LEDs on</Capability>
 </Channel>

 <!-- Master RGBW + color systems -->
