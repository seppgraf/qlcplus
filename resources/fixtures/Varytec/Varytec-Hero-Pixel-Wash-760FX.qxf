 <Channel Name="Tilt" Default="127" Preset="PositionTilt"/>
 <Channel Name="Tilt Fine" Preset="PositionTiltFine"/>
 <Channel Name="Pan/Tilt Speed" Preset="SpeedPanTiltFastSlow"/>

 <!-- Optics / Zoom -->
 <Channel Name="Zoom" Preset="BeamZoomSmallBig"/>
 <Channel Name="Zoom Rotation">
  <Group Byte="0">Prism</Group>
  <Capability Min="0" Max="127">Static position of the rotating lens unit (0 to 60 Â°)</Capability>
